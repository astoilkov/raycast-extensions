{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "instant-domain-search",
  "title": "Instant Domain Search",
  "description": "Find domains, check availability, search popular extensions, and buy premium domains.",
  "icon": "extension-icon.png",
  "author": "instantdomainsearch",
  "platforms": [
    "macOS",
    "Windows"
  ],
  "categories": [
    "Data",
    "Developer Tools",
    "Productivity",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search-domains",
      "title": "Search Domains",
      "description": "Check domain availability, find popular extensions, and buy premium domains.",
      "mode": "view"
    }
  ],
  "tools": [
    {
      "name": "search-domains",
      "title": "Search Domains",
      "description": "Check domain availability, find popular extensions, and buy premium domains."
    }
  ],
  "evals": [
    {
      "input": "@instant-domain-search Domain ideas for productivity geeks",
      "mocks": {
        "search-domains": [
          {
            "action": "https://instantdomainsearch.com/get/productivitygeeks.com",
            "availability": "taken",
            "domain": "productivitygeeks.com"
          },
          {
            "action": "https://instantdomainsearch.com/get/geekyproductivity.com",
            "availability": "available",
            "domain": "geekyproductivity.com"
          },
          {
            "action": "https://instantdomainsearch.com/get/productivityhacks.com",
            "availability": "taken",
            "domain": "productivityhacks.com"
          },
          {
            "action": "https://instantdomainsearch.com/get/geekytools.com",
            "availability": "taken",
            "domain": "geekytools.com"
          },
          {
            "action": "https://instantdomainsearch.com/get/efficiencygeeks.com",
            "availability": "aftermarket",
            "domain": "efficiencygeeks.com",
            "price": 78800
          },
          {
            "action": "https://instantdomainsearch.com/get/productivityninja.com",
            "availability": "aftermarket",
            "domain": "productivityninja.com",
            "price": 29900
          },
          {
            "action": "https://instantdomainsearch.com/get/geekyworkflows.com",
            "availability": "available",
            "domain": "geekyworkflows.com"
          },
          {
            "action": "https://instantdomainsearch.com/get/productivityhub.com",
            "availability": "aftermarket",
            "domain": "productivityhub.com",
            "price": 249900
          },
          {
            "action": "https://instantdomainsearch.com/get/smartproductivity.com",
            "availability": "taken",
            "domain": "smartproductivity.com"
          },
          {
            "action": "https://instantdomainsearch.com/get/geekproductivitytips.com",
            "availability": "available",
            "domain": "geekproductivitytips.com"
          }
        ]
      },
      "expected": [
        {
          "callsTool": {
            "arguments": {
              "domains": {
                "eq": [
                  "productivitygeeks.com",
                  "geekyproductivity.com",
                  "productivityhacks.com",
                  "geekytools.com",
                  "efficiencygeeks.com",
                  "productivityninja.com",
                  "geekyworkflows.com",
                  "productivityhub.com",
                  "smartproductivity.com",
                  "geekproductivitytips.com"
                ]
              }
            },
            "name": "search-domains"
          }
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.103.1",
    "@raycast/utils": "^1.17.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\`prepublishOnly\\` script and rerun \\`npm publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`npm run publish\\` instead.\\n\\n\" && exit 1",
    "publish": "npx @raycast/api@latest publish"
  }
}
